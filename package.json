{
  "name": "h5p-dictation",
  "version": "1.2.2",
  "description": "Let your students train their listening comprehension and spelling skills.",
  "scripts": {
    "test": "jest",
    "lint": "stylelint './src/**/*.scss' && eslint './src/**/*.js'",
    "build": "cross-env NODE_ENV=production webpack",
    "watch": "cross-env NODE_ENV=development webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otacke/h5p-dictation.git"
  },
  "keywords": [],
  "author": "Oliver Tacke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otacke/h5p-dictation/issues"
  },
  "homepage": "https://github.com/otacke/h5p-dictation#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/stylesheets/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/stylesheets/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^39.6.4",
    "jest": "^29.3.1",
    "mini-css-extract-plugin": "^2.7.2",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead"
}
